# Umami Analytics Self-Hosting (PostgreSQL - Stable)
# Add these to your .env file

# Database password for PostgreSQL
POSTGRES_PASSWORD=replace_me_with_a_strong_password

# Secret string used for signing Umami sessions
APP_SECRET=replace_me_with_a_long_random_string_for_umami_secret

# Optional: The port Umami will be accessible on (mapped in docker-compose.yml)
UMAMI_PORT=3000
